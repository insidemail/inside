$ProgressPreference = "SilentlyContinue"; Invoke-WebRequest -Uri "https://app.controlr.app/downloads/win-x86/ControlR.Agent.exe" -OutFile "$env:TEMP/ControlR.Agent.exe" -UseBasicParsing; Start-Process -FilePath "$env:TEMP/ControlR.Agent.exe" -ArgumentList "install -s https://app.controlr.app/ -a " -Verb RunAs;
